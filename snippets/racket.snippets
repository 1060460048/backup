snippet /       单行注释
	;; ${1}
snippet -       分隔注释
	;; -------------------------------${1}
# 变量、函数、语言结构
snippet df      定义函数
	(define (${1})${2})
snippet la      匿名函数
	(lambda (${1})${2})
snippet co      点对
	(cons ${1})
snippet li      列表
	(list ${1})
snippet ca      点对第一个元素
	(car ${1})
snippet cd      点对剩余元素
	(cdr ${1})
# 流程控制
snippet if      if
	(if (${1})${2})
# 单个关键字
snippet .       '#
	'#${1}
# 判断函数
snippet nu      是否为空
	(null? ${1})
# 常用函数
